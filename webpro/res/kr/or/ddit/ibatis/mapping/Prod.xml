<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="prod">
	<!-- list 가져오기 -->
	<select id= "selectLprod" resultClass="lprod">
       select * from lprod
   	</select>
   	
   	<select id= "searchByLgu" parameterClass="String" resultClass="prod">
       select prod_id,prod_name from prod where prod_lgu=#prod_lgu#
   	</select>
   	
   	<select id="selectById" parameterClass="String" resultClass="prod">
   		select* from prod where prod_id=#prod_id#
   	</select>
	
	
</sqlMap>